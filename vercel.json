{
  "version": 2,
  "builds": [
    {
      "src": "src/api/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "src/**/*.ts",
      "use": "@vercel/static-build"
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://vercel.com/1m4nims-projects/blog/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
