{
  "version": 2,
  "builds": [
    {
      "src": "src/api/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "src/**/*.{ts,tsx}",
      "use": "@vercel/static-build"
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://blog-git-main-1m4nims-projects.vercel.app/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
